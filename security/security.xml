<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Группын ангилал -->
        <record model="ir.module.category" id="tracking_category">
            <field name="name">Tracking</field>
            <field name="sequence">1</field>
        </record>
        <!-- Групп нэр -->
        <record id="group_tracking_admin" model="res.groups">
            <field name="name">Admin</field>
            <field name="category_id" ref="tracking.tracking_category" />
        </record>
        <record id="group_tracking_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="tracking.tracking_category" />
        </record>
    </data>
    <data noupdate="1">
        <record model="ir.rule" id="stock_picking_rule">
            <field name="name">stock_picking multi-company</field>
            <field name="model_id" search="[('model','=','stock.picking')]" model="ir.model"/>
            <field name="domain_force">[('company_id', 'in', company_ids)]</field>
        </record>
        <record model="ir.rule" id="stock_vehicle_comp_rule">
            <field name="name">Vehicle multi-company</field>
            <field name="model_id" ref="model_stock_vehicle"/>
            <field name="domain_force">[('company_id', 'in', company_ids)]</field>
        </record>
        <record model="ir.rule" id="stock_contract_comp_rule">
            <field name="name">Contract multi-company</field>
            <field name="model_id" ref="model_stock_contract"/>
            <field name="domain_force">[('company_id', 'in', company_ids)]</field>
        </record>
        <record model="ir.rule" id="stock_warehouse_comp_rule">
            <field name="name">Warehouse multi-company</field>
            <field name="model_id" ref="model_stock_warehouse"/>
            <field name="domain_force">[('company_id', 'in', company_ids)]</field>
        </record>
        <record model="ir.rule" id="stock_location_comp_rule">
            <field name="name">Location multi-company</field>
            <field name="model_id" ref="model_stock_location"/>
            <field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
        </record>
    </data>
</odoo>